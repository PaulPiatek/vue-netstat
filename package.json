{
  "name": "vue-netstat",
  "version": "1.0.0",
  "description": "A netstat viewer build with Electron and Vue",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Netstat",
    "Windows",
    "Electron",
    "Vue"
  ],
  "author": "Paul Piatek",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-builder": "^19.56.0",
    "electron-rebuild": "^1.7.3"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "multimap": "^1.0.2",
    "net-ping": "^1.2.1",
    "uuid": "^3.2.1",
    "vue": "^2.5.13",
    "windows-netstat": "^1.0.0"
  },
  "build": {
    "appId": "vue.netstat",
    "win": {
      "target": "7z"
    }
  }
