{
  "name": "vue-netstat",
  "version": "1.0.0",
  "description": "A netstat viewer build with Electron and Vue",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Netstat",
    "Windows",
    "Electron",
    "Vue"
  ],
  "author": "Paul Piatek",
  "license": "MIT",
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-builder": "^22.11.1",
    "electron-rebuild": "^2.3.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "multimap": "^1.1.0",
    "ping": "^0.4.1",
    "uuid": "^8.3.2",
    "vue": "^2.6.12",
    "windows-netstat": "^1.0.1"
  },
  "build": {
    "appId": "vue.netstat",
    "win": {
      "target": "7z"
    }
  }
}
